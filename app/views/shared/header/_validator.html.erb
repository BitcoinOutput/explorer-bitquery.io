<% content_for :title, t( "tabs.#{controller_name}.#{params[:action]}.title", network: @network[:name], index: @index) %>
<% content_for :description, t( "tabs.#{controller_name}.#{params[:action]}.description", network: @network[:name], index: @index) %>
<% content_for :keywords, t( "tabs.#{controller_name}.#{params[:action]}.keywords", network: @network[:name], index: @index) %>

<%= render partial: 'shared/title', locals: {
          icon: t("tabs.#{controller_name}.icon"),
          header: t( "tabs.#{controller_name}.header", network: @network[:name], index: @index ).html_safe,
          reportrange: true} %>
