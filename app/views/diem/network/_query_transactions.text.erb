query ($network: DiemNetwork, $dateFormat: String!, $from: ISO8601DateTime, $till: ISO8601DateTime){
  chain: diem(network: $network){
    transactions(options:{asc: "date.month"}, date: {since: $from, till: $till }) {
      date{
        month: date(format: $dateFormat)
      }
      count
    }
  }
}